{
  "hosting": {
    "public": "dist/accounting-books/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": [
    {
      "bucket": "accountingbooks-9fa26.firebasestorage.app",
      "rules": "storage.rules"
    }
  ]
}
